{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "tsconfig.json",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "prefer-const": 1,
    "prefer-destructuring": 1,
    "no-alert": 1,
    "no-unused-vars": 0,
    "no-prototype-builtins": 1,
    "no-console": 1,
    "getter-return": 1,
    "no-unreachable": 1,
    "no-var": 1,
    "no-else-return": 1,
    "no-return-await": 1,
    "prefer-arrow-callback": 1,
    "no-duplicate-imports": 1,
    "no-const-assign": 1,
    "eol-last": [1, "always"],
    "object-curly-spacing": [1, "always"],
    "array-bracket-spacing": [1, "always"],
    "semi": [1, "always"],
    "quotes": [1, "single"],
    "arrow-spacing": [
      1,
      {
        "before": true,
        "after": true
      }
    ],
    "arrow-body-style": 0,
    "camelcase": 1,
    "comma-dangle": 1,
    "complexity": 0,
    "constructor-super": 1,
    "dot-notation": 1,
    "eqeqeq": [1, "always"],
    "guard-for-in": 0,
    "id-match": 1,
    "max-classes-per-file": [1, 1],
    "max-len": [
      1,
      {
        "code": 140
      }
    ],
    "new-parens": 1,
    "no-bitwise": 1,
    "no-caller": 1,
    "no-cond-assign": 1,
    "no-debugger": 1,
    "no-duplicate-case": 1,
    "no-empty": 1,
    "no-eval": 1,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-invalid-this": 0,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-redeclare": 1,
    "no-sequences": 0,
    "no-shadow": [
      1,
      {
        "hoist": "all"
      }
    ],
    "no-sparse-arrays": 1,
    "no-template-curly-in-string": 1,
    "no-throw-literal": 1,
    "no-trailing-spaces": 1,
    "no-undef-init": 1,
    "no-underscore-dangle": [1, { "allow": [ "_id" ] }],
    "no-unsafe-finally": 1,
    "no-unused-expressions": 1,
    "no-unused-labels": 1,
    "object-shorthand": 1,
    "one-var": [1, "never"],
    "prefer-object-spread": 1,
    "radix": 1,
    "space-in-parens": [1, "never"],
    "spaced-comment": 1,
    "use-isnan": 1,
    "valid-typeof": 0,
    "@typescript-eslint/adjacent-overload-signatures": 1,
    "@typescript-eslint/array-type": 1,
    "@typescript-eslint/ban-types": 1,
    "@typescript-eslint/class-name-casing": 1,
    "@typescript-eslint/consistent-type-assertions": 1,
    "@typescript-eslint/interface-name-prefix": 1,
    "@typescript-eslint/member-delimiter-style": [
      1,
      {
        "multiline": {
          "delimiter": "semi",
          "requireLast": true
        },
        "singleline": {
          "delimiter": "comma",
          "requireLast": false
        }
      }
    ],
    "@typescript-eslint/no-empty-function": 1,
    "@typescript-eslint/no-empty-interface": 1,
    "@typescript-eslint/no-explicit-any": 0,
    "@typescript-eslint/no-misused-new": 1,
    "@typescript-eslint/no-namespace": 1,
    "@typescript-eslint/no-parameter-properties": 0,
    "@typescript-eslint/no-require-imports": 1,
    "@typescript-eslint/no-this-alias": 1,
    "@typescript-eslint/no-unnecessary-type-assertion": 1,
    "@typescript-eslint/no-use-before-define": 0,
    "@typescript-eslint/no-var-requires": 1,
    "@typescript-eslint/prefer-for-of": 1,
    "@typescript-eslint/prefer-function-type": 1,
    "@typescript-eslint/prefer-namespace-keyword": 1,
    "@typescript-eslint/prefer-readonly": 1,
    "@typescript-eslint/promise-function-async": 1,
    "@typescript-eslint/quotes": [1, "single"],
    "@typescript-eslint/semi": [1, "always"],
    "@typescript-eslint/triple-slash-reference": 1,
    "@typescript-eslint/unified-signatures": 1
  }
}
